package mx.gob.stcm.credencial.accesoDB.mapeo;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "roles")
public class RolUsuario {
	@Id
	@Column(name = "id_rol")
	private Integer id;
	
	@Column(name = "rol")
	private String rol;
}
