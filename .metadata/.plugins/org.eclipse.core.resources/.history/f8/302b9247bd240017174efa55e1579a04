package mx.gob.stcm.credencial.accesoDB.mapeo;

import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.JoinColumn;
import javax.persistence.OneToOne;
import javax.persistence.Table;

@Entity
@Table(name = "beneficiario")
public class Beneficiario {
	@Id
	@Column(name = "curp")
	private String id;
	
	@Column(name = "nombre")
	private String nombre;
	
	@Column(name = "paterno")
	private String apellidoPaterno;
	
	@Column(name = "materno")
	private String apellidoMaterno;
	
	@Column(name = "telefono_casa")
	private String telefono;
	
	@Column(name = "fecha_nacimiento")
	private Date fechaNacimiento;
	
	@Column(name = "fecha_registro")
	private Date fechaRegistro;
	
	@Column(name = "correo_e")
	private String correoElectronico;
	
	@Column(name = "nalle_num")
	private String calleNumero;
	
	@Column(name = "codigo_p")
	private Integer codigoPostal;
	
	@Column(name = "observacion")
	private String observaciones;
	
	@Column(name = "sanguineo")
	private Integer sanguineo;
	
	@Column(name = "folio")
	private String folio;
	
	@Column(name = "contacto")
	private Integer contacto;

	@Column(name = "foto")
	private String foto;
	
	@Column(name = "carpeta")
	private String carpeta;
	
	@OneToOne(optional = false, mappedBy = "beneficiario")
	@JoinColumn(name = "contacto", referencedColumnName = "id_contacto", insertable = false, updatable = false)
	private Contacto contactoObj;
	
	@OneToOne(optional = false, mappedBy = "beneficiario")
	@JoinColumn(name = "sanguieno", referencedColumnName = "id_sangre", insertable = false, updatable = false)
	private Sanguineo sanguienoObj;
	
	@OneToOne(optional = false, mappedBy = "beneficiario")
	@JoinColumn(name = "codigo_p", referencedColumnName = "id_cp", insertable = false, updatable = false)
	private CodigoPostal codigoPostalObj;

	/**
	 * @return the id
	 */
	public String getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(String id) {
		this.id = id;
	}

	/**
	 * @return the nombre
	 */
	public String getNombre() {
		return nombre;
	}

	/**
	 * @param nombre the nombre to set
	 */
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	/**
	 * @return the apellidoPaterno
	 */
	public String getApellidoPaterno() {
		return apellidoPaterno;
	}

	/**
	 * @param apellidoPaterno the apellidoPaterno to set
	 */
	public void setApellidoPaterno(String apellidoPaterno) {
		this.apellidoPaterno = apellidoPaterno;
	}

	/**
	 * @return the apellidoMaterno
	 */
	public String getApellidoMaterno() {
		return apellidoMaterno;
	}

	/**
	 * @param apellidoMaterno the apellidoMaterno to set
	 */
	public void setApellidoMaterno(String apellidoMaterno) {
		this.apellidoMaterno = apellidoMaterno;
	}

	/**
	 * @return the telefono
	 */
	public String getTelefono() {
		return telefono;
	}

	/**
	 * @param telefono the telefono to set
	 */
	public void setTelefono(String telefono) {
		this.telefono = telefono;
	}

	/**
	 * @return the fechaNacimiento
	 */
	public Date getFechaNacimiento() {
		return fechaNacimiento;
	}

	/**
	 * @param fechaNacimiento the fechaNacimiento to set
	 */
	public void setFechaNacimiento(Date fechaNacimiento) {
		this.fechaNacimiento = fechaNacimiento;
	}

	/**
	 * @return the fechaRegistro
	 */
	public Date getFechaRegistro() {
		return fechaRegistro;
	}

	/**
	 * @param fechaRegistro the fechaRegistro to set
	 */
	public void setFechaRegistro(Date fechaRegistro) {
		this.fechaRegistro = fechaRegistro;
	}

	/**
	 * @return the correoElectronico
	 */
	public String getCorreoElectronico() {
		return correoElectronico;
	}

	/**
	 * @param correoElectronico the correoElectronico to set
	 */
	public void setCorreoElectronico(String correoElectronico) {
		this.correoElectronico = correoElectronico;
	}

	/**
	 * @return the calleNumero
	 */
	public String getCalleNumero() {
		return calleNumero;
	}

	/**
	 * @param calleNumero the calleNumero to set
	 */
	public void setCalleNumero(String calleNumero) {
		this.calleNumero = calleNumero;
	}

	/**
	 * @return the codigoPostal
	 */
	public Integer getCodigoPostal() {
		return codigoPostal;
	}

	/**
	 * @param codigoPostal the codigoPostal to set
	 */
	public void setCodigoPostal(Integer codigoPostal) {
		this.codigoPostal = codigoPostal;
	}

	/**
	 * @return the observaciones
	 */
	public String getObservaciones() {
		return observaciones;
	}

	/**
	 * @param observaciones the observaciones to set
	 */
	public void setObservaciones(String observaciones) {
		this.observaciones = observaciones;
	}

	/**
	 * @return the sanguineo
	 */
	public Integer getSanguineo() {
		return sanguineo;
	}

	/**
	 * @param sanguineo the sanguineo to set
	 */
	public void setSanguineo(Integer sanguineo) {
		this.sanguineo = sanguineo;
	}

	/**
	 * @return the folio
	 */
	public String getFolio() {
		return folio;
	}

	/**
	 * @param folio the folio to set
	 */
	public void setFolio(String folio) {
		this.folio = folio;
	}

	/**
	 * @return the contacto
	 */
	public Integer getContacto() {
		return contacto;
	}

	/**
	 * @param contacto the contacto to set
	 */
	public void setContacto(Integer contacto) {
		this.contacto = contacto;
	}

	/**
	 * @return the foto
	 */
	public String getFoto() {
		return foto;
	}

	/**
	 * @param foto the foto to set
	 */
	public void setFoto(String foto) {
		this.foto = foto;
	}

	/**
	 * @return the carpeta
	 */
	public String getCarpeta() {
		return carpeta;
	}

	/**
	 * @param carpeta the carpeta to set
	 */
	public void setCarpeta(String carpeta) {
		this.carpeta = carpeta;
	}

	/**
	 * @return the contactoObj
	 */
	public Contacto getContactoObj() {
		return contactoObj;
	}

	/**
	 * @param contactoObj the contactoObj to set
	 */
	public void setContactoObj(Contacto contactoObj) {
		this.contactoObj = contactoObj;
	}

	/**
	 * @return the sanguienoObj
	 */
	public Sanguineo getSanguienoObj() {
		return sanguienoObj;
	}

	/**
	 * @param sanguienoObj the sanguienoObj to set
	 */
	public void setSanguienoObj(Sanguineo sanguienoObj) {
		this.sanguienoObj = sanguienoObj;
	}

	/**
	 * @return the codigoPostalObj
	 */
	public CodigoPostal getCodigoPostalObj() {
		return codigoPostalObj;
	}

	/**
	 * @param codigoPostalObj the codigoPostalObj to set
	 */
	public void setCodigoPostalObj(CodigoPostal codigoPostalObj) {
		this.codigoPostalObj = codigoPostalObj;
	}
}
